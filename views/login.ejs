          Saturday Closed
          <hr class="visible-xs">
        </section>
        <section id="address" class="col-sm-4">
          <span>Contact Form:</span><br>
          <div class="input-group">
            <input class="input-group-field" type="text" placeholder="Name"><br>
            <br>
            <input class="input-group-field" type="email" placeholder="Email address"><br>
            <br>
            <textarea placeholder="Message"></textarea><br>
          </div>
          <a class="button expanded" href="#">Submit</a>
          <hr class="visible-xs">
        </section>
        <section id="address" class="col-sm-4">
          <span>Contact details:</span><br>
          <a href="tel:+919199110501">919919919</a><br>
          Delhi Technological University, Shahbad Daulatpur<br>
          Main Bawana Road, DL - 42, IN<br>
          New Delhi
          <hr class="visible-xs">
        </section>
      </div>
